{
  "name": "@cznethub/cznet-vue-core",
  "version": "0.1.91",
  "private": false,
  "keywords": [
    "cznet",
    "vue",
    "vue 3",
    "vuetify",
    "form",
    "jsonforms",
    "jsonschema",
    "schema",
    "uischema"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --mode dev",
    "build": "npm run clean:dist && vite build --mode prod && vue-tsc --declaration --emitDeclarationOnly",
    "publish": "npm publish --access public",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run clean:coverage && vitest --coverage --mode test",
    "up": "taze major -I"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@jsonforms/core": "^3.2.1",
    "@jsonforms/vue": "^3.2.1",
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.0.0 || >=3.0.0",
    "vue-facing-decorator": "^3.0.4",
    "vuetify": "^3.5.6"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@jsonforms/core": "^3.2.1",
    "@jsonforms/vue": "^3.2.1",
    "ajv-errors": "^3.0.0",
    "ajv-keywords": "^5.1.0",
    "dayjs": "^1.11.10",
    "google-maps": "^4.3.3",
    "pretty-bytes": "^6.1.1",
    "v-mask": "^2.3.0",
    "vue-demi": "latest",
    "vue-easy-dnd": "^2.1.3",
    "vue-facing-decorator": "^3.0.4",
    "vuetify": "^3.5.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@mdi/font": "^7.4.47",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "ajv": "^8.12.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "latest",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "happy-dom": "^14.3.6",
    "lodash-es": "^4.17.21",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "stylelint": "^16.3.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "taze": "^0.13.3",
    "typescript": "latest",
    "vite": "^5.2.6",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cznethub/cznet-vue-core.git"
  },
  "author": "Maurier Ramirez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cznethub/cznet-vue-core/issues"
  },
  "homepage": "https://github.com/cznethub/cznet-vue-core#readme"
}
