{
  "type": "VerticalLayout",
  "elements": [
    {
      "type": "Control",
      "scope": "#/properties/@context",
      "rule": {
        "effect": "HIDE",
        "condition": {}
      }
    },
    {
      "type": "Group",
      "label": "Basic Information",
      "elements": [
        {
          "type": "Control",
          "scope": "#/properties/name"
        },
        {
          "type": "HorizontalLayout",
          "elements": [
            {
              "type": "Control",
              "scope": "#/properties/url"
            },
            {
              "type": "Control",
              "scope": "#/properties/@type"
            }
          ]
        },
        {
          "type": "Control",
          "scope": "#/properties/description",
          "options": {
            "multi": true
          }
        },
        {
          "type": "Control",
          "scope": "#/properties/keywords"
        },
        {
          "type": "Control",
          "scope": "#/properties/variableMeasured"
        },
        {
          "type": "HorizontalLayout",
