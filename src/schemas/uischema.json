                      }
                    }
                  ]
                },
                {
                  "type": "Control",
                  "scope": "#/properties/geo",
                  "options": {
                    "detail": {
                      "0": {
                        "type": "VerticalLayout",
                        "elements": [
                          {
                            "type": "Control",
                            "scope": "#/properties/@type",
                            "rule": {
                              "effect": "HIDE",
                              "condition": {}
                            }
                          },
                          {
                            "type": "Control",
                            "scope": "#/properties/line"
                          }
                        ]
                      },
                      "1": {
                        "type": "VerticalLayout",
                        "elements": [
                          {
                            "type": "Control",
                            "scope": "#/properties/@type",
                            "rule": {
                              "effect": "HIDE",
                              "condition": {}
                            }
                          },
                          {
                            "type": "Control",
                            "scope": "#/properties/polygon"
                          }
                        ]
                      },
                      "2": {
                        "type": "MapLayout",
                        "options": {
                          "map": {
                            "type": "point",
                            "north": "latitude",
                            "east": "longitude"
                          }
                        },
                        "elements": [
                          {
                            "type": "Control",
                            "scope": "#/properties/@type",
                            "rule": {
                              "effect": "HIDE",
                              "condition": {}
                            }
                          },
                          {
                            "type": "Control",
                            "scope": "#/properties/latitude"
                          },
                          {
                            "type": "Control",
                            "scope": "#/properties/longitude"
                          }
                        ]
                      }
                    }
                  }
                }
              ]
            }
          }
        }
      ]
    },
