        },
        {
          "type": "Control",
          "scope": "#/properties/creativeWorkStatus"
        }
      ]
    },
    {
      "type": "Control",
      "scope": "#/properties/creator",
      "options": {
        "detail": {
          "type": "Control",
          "scope": "#",
          "options": {
            "flat": true,
            "detail": {
              "0": {
                "type": "VerticalLayout",
                "elements": [
                  {
                    "type": "Control",
                    "scope": "#/properties/@type",
                    "rule": {
                      "effect": "HIDE",
                      "condition": {}
                    }
                  },
                  {
                    "type": "HorizontalLayout",
                    "elements": [
                      {
                        "type": "Control",
                        "scope": "#/properties/name"
                      },
                      {
                        "type": "Control",
                        "scope": "#/properties/email"
                      }
                    ]
                  },
                  {
                    "type": "Control",
                    "scope": "#/properties/identifier"
                  }
                ]
              },
              "1": {
                "type": "VerticalLayout",
                "elements": [
                  {
                    "type": "Control",
                    "scope": "#/properties/@type",
                    "rule": {
                      "effect": "HIDE",
                      "condition": {}
                    }
                  },
                  {
                    "type": "HorizontalLayout",
                    "elements": [
                      {
                        "type": "Control",
                        "scope": "#/properties/name"
                      },
                      {
                        "type": "Control",
                        "scope": "#/properties/url"
                      }
                    ]
                  },
                  {
                    "type": "HorizontalLayout",
                    "elements": [
                      {
                        "type": "Control",
                        "scope": "#/properties/identifier"
                      },
                      {
                        "type": "Control",
                        "scope": "#/properties/address"
                      }
                    ]
                  }
                ]
              }
            }
          }
        }
      }
    },
    {
      "type": "Control",
      "scope": "#/properties/identifier",
      "options": {
        "useTableLayout": true,
        "showSortButtons": true
      }
    },
    {
      "type": "Control",
      "scope": "#/properties/distribution",
      "options": {
        "elementLabelProp": [
          "name"
        ],
        "detail": {
          "type": "VerticalLayout",
          "elements": [
            {
              "type": "HorizontalLayout",
              "elements": [
                {
                  "type": "Control",
                  "scope": "#/properties/name"
                },
                {
                  "type": "Control",
                  "scope": "#/properties/contentUrl"
                }
              ]
            },
            {
              "type": "HorizontalLayout",
              "elements": [
                {
                  "type": "Control",
                  "scope": "#/properties/encodingFormat"
                },
                {
                  "type": "Control",
